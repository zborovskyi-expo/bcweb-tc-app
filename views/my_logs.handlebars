{{#ifNotEquals time_start ""}}
  <div class="clock" id="clock_content">
    {{time_start}}
  </div>
{{/ifNotEquals}}


<h1 class="page-header">{{ title }}</h1>
<div class="row">
  <div class="col-lg-12">
    {{#each logs }}
      {{#if this.beforeBreak }}
        <div class="panel panel-default panel-m-top">
          <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-calendar" aria-hidden="true"></i> {{this.monthName}}</h3>
          </div>
          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-bordered table-hover table-striped">
                <thead>
                  <tr>
                    <th>Date</th>
                    <th>Time Start</th>
                    <th>Time Over</th>
                    <th>Username</th>
                    <th>Status</th>
                    <th>Sum time</th>
                  </tr>
                </thead>
                <tbody>
      {{/if}}
        <tr {{#if this.weekend }}class="weekend"{{/if}}>
          <td>{{this.date}}</td>
          <td>{{this.time_start}}</td>
          <td>{{this.time_over}}</td>
          <td>{{this.username}}</td>
          <td>{{this.status}}</td>
          <td>{{this.sum_time}}</td>
        </tr>
      {{#if this.afterBreak }}
                  <tr>
                    <td colspan="6" class="all_time">Suma godzin: {{this.all_time}}</td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div><!-- /panel-body -->
        </div><!-- /panel -->
      {{/if}}
    {{/each}}
  </div>
</div>
